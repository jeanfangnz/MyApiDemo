{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Rounding": {
    "DefaultDecimals": 2,
    "Mode": "AwayFromZero",
    "PerCurrency": {
      "JPY": 0,
      "USD": 2,
      "KWD": 3
    }
  },
  "ExchangeRate": {
    "BaseUrl": "https://open.er-api.com/v6/",
    "TimeoutSeconds": 5,
    "Strategy": "CachedLive",
    "CacheTtlSeconds": 300,
    "Providers": {
      "Primary": "Fixer",
      "Fallback": "Mock"
    }
  }
}